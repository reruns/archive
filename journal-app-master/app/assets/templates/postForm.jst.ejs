<form id="post_form">
  <label for="title">Title: </label>
  <input type="text" name="post[title]" value="<%= post.get('title') %>"><br><br>
  <label for="body">Body: </label><br>
  <textarea name="post[body]" rows="8" cols="40"><%= post.get('body') %></textarea>
  <br>
  <button id="post_form_button">Create post!</button>
</form>
